{
  "name": "Event_Attandance_Api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    ".": "nodemon --watch src --watch libs ./src/server.ts",
    "start": "npm run build && nodemon ./dist/server.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "watch-node": "nodemon --inspect=0.0.0.0:9229 .",
    "build": "tsc --build .",
    "build-ts": "ttsc",
    "watch-ts": "tsc -w",
    "delete": "pm2 delete task-management-app",
    "stop": "pm2 stop task-management-app",
    "lint": "tsc --noEmit && eslint \"./src/*.{js,ts}\" --quiet",
    "test": "NODE_ENV=testing jest --no-cache --coverage --silent --passWithNoTests --detectOpenHandles --forceExit",
    "migration:create": "ts-node -r ./src/register-module-alias.ts ./node_modules/typeorm/cli.js --config src/ormconfig-cli.ts migration:create",
    "migration:generate": "ts-node -r ./src/register-module-alias.ts ./node_modules/typeorm/cli.js --config src/ormconfig-cli.ts migration:generate",
    "migration:up": "ts-node -r ./src/register-module-alias.ts ./node_modules/typeorm/cli.js migration:run -d ./src/databases/dataSource.ts",
    "migration:revert": "ts-node -r ./src/register-module-alias.ts ./node_modules/typeorm/cli.js --config src/ormconfig-cli.ts migration:revert",
    "test:ci": "npm run test",
    "test:compose": "docker-compose up --abort-on-container-exit --build"
  },
  "keywords": [
    "node.js",
    "javascript",
    "typescript",
    "express",
    "mysql",
    "typeorm"
  ],
  "author": "FRIDAY ONOJAH <fridayonojah360@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.1.4",
    "eslint-config-prettier": "^8.8.0",
    "event-stream": "^4.0.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mysql2": "^3.3.3",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "ts-node": "^10.9.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.16",
    "typeorm-typedi-extensions": "^0.4.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.3",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.1.3",
    "typescript-transform-paths": "^3.4.6"
  }
}
